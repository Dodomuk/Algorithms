package somansa;

import java.util.Scanner;

public class Decimal {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		flg(num);

	}

	public static void flg(int num) {
		while (true) {
			for (int n = 2; n < num; n++) {

				if (num % n == 0) {
					System.out.println("소수가 아닙니다");
					break;

				} else {
					System.out.println("소수입니다");
					break;
				}
			}
		}
	}
}
