package somansa;

import java.util.Scanner;

public class Decimal {

   static Scanner sc = new Scanner(System.in);
	  int num = sc.nextInt();
	   
   public static void main(String[] args) {
	
	   flg(num);
	   
}
	public void flg(num){
   while(true) {
   for(int n=2;n<num;n++) {
       
   if(num % n == 0) {
      System.out.println("소수가 아닙니다");
      break;
   }
   }
   System.out.println("소수입니다");
   break;
   }
	}
}
